        "import numpy as np\n",
        "\n",
        "A = np.array([[0, 1],             [2, 3],\n",
        "              [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3],\n",
        "              [4, 5, 6, 7]])\n",
        "\n",
        "C = np.dot(A, B)\n",
        "\n",
        "print(\"２つの行列の積:\")\n",
        "print(C)\n",
        "\n",
        "max_value = np.max(C)\n",
        "print(\"２つの行列の積の最大値:\", max_value)"
